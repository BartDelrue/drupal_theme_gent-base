name = Gent starterkit
description = Gent default starterkit for Gent base theme.
base theme = gent_base
core = 7.x
screenshot = screenshot.png

; ========================================
; Regions
; ========================================
regions[search] = Search
regions[top_menu] = Top menu
regions[header] = Header
regions[site_name] = Site name
regions[navigation] = Navigation
regions[content_top] = Top content
regions[content] = Content
regions[sidebar] = Sidebar
regions[content_bottom] = Bottom content
regions[footer_top] = Top footer
regions[footer] = Footer
regions[footer_bottom] = Footer bottom

; ========================================
; Styles
; ========================================
stylesheets[all][] = css/default_starterkit.styles.css
stylesheets[print][] = css/default_starterkit.print.css

; ========================================
; Scripts
; ========================================
; Uncomment if you have a minified version of the base theme's theme.js in your subtheme (grunt build will create one)
;scripts[] = js/theme.min.js

; ========================================
; Default Theme settings
; ========================================
settings[top_menu_render_method] = region

